{
  "name": "react-animate-height",
  "version": "2.1.2",
  "description": "Lightweight React component for animating height using CSS transitions.",
  "main": "lib/AnimateHeight.js",
  "types": "lib/index.d.ts",
  "author": "Stanko",
  "homepage": "https://github.com/Stanko/react-animate-height",
  "repository": {
    "type": "git",
    "url": "https://github.com/Stanko/react-animate-height.git"
  },
  "bugs": {
    "url": "https://github.com/Stanko/react-animate-height/issues"
  },
  "scripts": {
    "clean": "rm -rf ./dist-docs ./dist ./lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server",
    "build": "tsc -b .",
    "build-docs": "rm -rf ./dist-docs && EXAMPLE=\"true\" NODE_ENV=\"production\" webpack",
    "publish-to-npm": "run-s clean build && npm publish",
    "publish-docs": "sh publish-docs.sh",
    "publish-all": "npm run publish-to-npm && npm run publish-docs",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier source/* --write",
    "preview": "rm -rf ./dist && NODE_ENV=\"production\" webpack-dev-server",
    "update-yarn-sdks": "yarn dlx @yarnpkg/sdks vscode"
  },
  "devDependencies": {
    "@swc/core": "^1.2.168",
    "@types/events": "^3",
    "@types/node": "^17.0.25",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^16.14.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "events": "^3.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "@types/prop-types": ">15",
    "@types/react": ">16",
    "react": ">=15.6.2"
  },
  "peerDependenciesMeta": {
    "@types/prop-types": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "react": {
      "optional": false
    }
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "keywords": [
    "react",
    "react-component",
    "slide",
    "slide up",
    "slide down",
    "animate height"
  ],
  "license": "MIT",
  "packageManager": "yarn@3.2.0"
}
